    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.49",
    "sass-embedded": "^1.83.0",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
