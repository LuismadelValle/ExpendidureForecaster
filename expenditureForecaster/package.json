    "@typescript-eslint/parser": "^7.18.0",
    "axios": "^1.7.9",
    "chart.js": "^3.9.1",
    "core-js": "^3.39.0",
    "lodash": "^4.17.21",
    "primeicons": "^7.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-chart-3": "^3.1.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "^4.5.0",
    "vue-sidebar-menu": "^5.4.2",
    "vue3-charts": "^1.1.33",
    "vue3-table-lite": "^1.4.2",
    "vuetify": "^3.8.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
